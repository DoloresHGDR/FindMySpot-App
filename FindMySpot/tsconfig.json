{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".", 
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "noEmit": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "esModuleInterop": true,
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "services/storage.js"  ]
}
